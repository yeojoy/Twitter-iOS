# platform :ios, '9.0'

target 'TwitterTutorial' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TwitterTutorial
	pod 'FirebaseAnalytics'
	pod 'FirebaseAuth'
	# Firebase realtime db
  pod 'FirebaseDatabase'
	# Firebase storage
  pod 'FirebaseStorage'
	
	# Image Loader library
	pod 'SDWebImage', '~> 5.0'

end
